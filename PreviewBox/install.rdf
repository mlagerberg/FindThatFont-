<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>previewbox-findthatfont@monkeyinmysoup</em:id>
    
    <!-- Target Application: FindThatFont! -->
    <em:targetApplication>
      <Description>
        <em:id>findthatfont@monkeyinmysoup</em:id>
        <em:minVersion>0.2</em:minVersion>
        <em:maxVersion>1.*</em:maxVersion>
      </Description>
    </em:targetApplication>
    
    <em:name>PreviewBox</em:name>
    <em:version>2.1</em:version>
    <em:creator>Mathijs Lagerberg</em:creator>
    <em:description>Adds a 'Lorem Ipsum' preview box to the interface, and a preview of individual characters and symbols</em:description>
    <em:homepageURL>https://github.com/monkeyinmysoup/FindThatFont-</em:homepageURL>
    <em:iconURL>chrome://previewbox/skin/icon.png</em:iconURL>
    
    <em:file>
      <Description about="urn:mozilla:extension:file:previewbox.jar">
        <em:package>content/</em:package>
        <em:locale>locale/en-US/</em:locale>
	    <em:skin>skin/</em:skin>
      </Description>
    </em:file>

  </Description>
</RDF>